<div class="dark-background-panel">
    <div class="center-panel">
        <div class="column-block">
            <div class="column-block-column-fill">
                <div class="app-bar-logo yakjs-logo-white"></div>
            </div>
            <div class="column-block-column-fill" style="vertical-align: bottom">
                <div class="app-bar-badges">
                    <a class="badge" href="http://www.yakjs.com" target="_blank">
                        <span class="badge-label">DOC</span>
                        <span class="badge-value-highlight">www.yakjs.com</span>
                    </a>

                    {{#versionInfo.isNewVersionAvailable}}
                    <a class="badge" href="http://www.yakjs.com" target="_blank">
                        <span class="badge-label">New Version</span>
                        <span class="badge-value-highlight">{{versionInfo.lastReleasedVersion}}</span>
                    </a>
                    {{/versionInfo.isNewVersionAvailable}}

                    <a class="badge" href="http://www.yakjs.com" target="_blank">
                        <span class="badge-label">Version</span>
                        <span class="badge-value">{{versionInfo.version}}</span>
                    </a>
                    <a class="badge" href="https://github.com/cschuller/yakjs" target="_blank">
                        <span class="badge-label">GitHub</span>
                        <span class="badge-value">cschuller/yakjs</span>
                    </a>
                    <a class="badge" href="https://github.com/cschuller/yakjs" target="_blank">
                        <span class="badge-label">License</span>
                        <span class="badge-value">MIT</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
